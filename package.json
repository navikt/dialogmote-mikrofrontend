{
  "name": "dialogmote-mikrofrontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(ts|tsx)\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --component"
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^1.3.9",
    "@grafana/faro-web-tracing": "^1.3.9",
    "@navikt/aksel-icons": "4.1.6",
    "@navikt/ds-react": "4.1.6",
    "@navikt/nav-dekoratoren-moduler": "^2.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.12",
    "styled-components": "^5.3.9",
    "swr": "2.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.4",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.2.58",
    "@types/react-dom": "18.2.19",
    "@types/react-modal": "3.16.3",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "^10.4.17",
    "cypress": "^13.6.6",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "husky": "7.0.0",
    "jsdom": "21.1.0",
    "lint-staged": "13.1.0",
    "msw": "^1.2.1",
    "postcss": "^8.4.35",
    "prettier": "2.8.8",
    "pretty-quick": "3.3.1",
    "rollup-plugin-import-map": "3.0.0",
    "typescript": "4.9.5",
    "uuid": "^9.0.1",
    "vite": "^4.5.2",
    "vite-plugin-css-injected-by-js": "2.4.0",
    "vitest": "0.28.3"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc.pre-commit.cjs --fix",
      "prettier --cache --write"
    ],
    "*.{css,md,html,json}": [
      "prettier --cache --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
